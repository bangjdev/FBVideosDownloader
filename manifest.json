{
  "name": "FB Videos Downloader",
  "version": "1.0",
  "description": "A helper chrome extension to download facebook's videos, even in private group",
  "content_scripts": [
  	{
  	  "js": ["jquery.js", "content.js"],
      "matches": [ "http://www.facebook.com/*/videos/*", "https://www.facebook.com/*/videos/*"]
  	}
  ],
  "page_action": {
    "default_popup": "menu.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "contextMenus", "tabs"
  ],
  "web_accessible_resources": ["download_icon.png"],
  "manifest_version": 2
}